#!"/home/yangliu/PycharmProjects/python crawler/venv/bin/python3.5"
# EASY-INSTALL-ENTRY-SCRIPT: 'pyspider==0.3.10','console_scripts','pyspider'
__requires__ = 'pyspider==0.3.10'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyspider==0.3.10', 'console_scripts', 'pyspider')()
    )
